<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:dist="http://schemas.android.com/apk/distribution"
          package="systems.autumnsky.linden_free">


    <dist:module dist:instant="true"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:name=".LindenFreeApp">
        <activity
                android:name="systems.autumnsky.linden_free.MedicineActivity"
                android:label="@string/title_activity_medicine">
        </activity>
        <activity
                android:name="systems.autumnsky.linden_free.LogActivity"
                android:label="@string/title_activity_log_view">
        </activity>
        <activity
                android:name="systems.autumnsky.linden_free.MainActivity"
                android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
</manifest>